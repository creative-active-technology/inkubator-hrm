	<parameter name="BIODATA_ID" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT bio_education_history.*,
education_level.name as "education_level_name",
institution_education.institution_education_name as "institution_education_name",
faculty.faculty_name as "faculty_name",
major.major_name as "major_name"
FROM bio_education_history
LEFT JOIN education_level ON bio_education_history.pendidikan_level_id=education_level.id
LEFT JOIN institution_education ON bio_education_history.institution_education_id=institution_education.id
LEFT JOIN faculty ON faculty.id=bio_education_history.faculty_id
LEFT JOIN major ON major.id=bio_education_history.major_id
WHERE biodata_id=$P{BIODATA_ID}
ORDER BY year_in DESC]]>
	</queryString>
